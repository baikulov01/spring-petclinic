pipeline {
    agent any
    stages {
	stage('Build'){
                steps{
			cd /d D:\petclinic\spring-petclinic
			mvn clean package
                }	
	    }
	stage('Archive'){
		steps{
			"C:\Program Files\WinRAR\WinRAR.exe" a -r D:\1Rar\target.rar D:\petclinic\spring-petclinic\target
		}
	}
	stage('Deploy'){
		steps{
			"C:\Program Files\WinRAR\WinRAR.exe" x D:\1Rar\target.rar D:\1Rar\Deploy
		}
	}
}
}
